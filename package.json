    },
    "scripts": {
        "checkoutMaster": "git checkout master",
        "mergeStag": "git merge --commit staging",
        "gitCommitMerge": "git commit -am \"Merge staging\"",
        "build": "gulp build",
        "addBuildOutputs": "git add ./public/*",
        "commitBuildOutputs": "git commit -m \"Committing build outputs\"",
        "origin": "git push origin master",
        "heroku": "git push heroku master",
        "checkoutStag": "git checkout staging",
        "deploy": "npm run checkoutMaster && npm run mergeStag && npm run build && npm run addBuildOutputs && npm run commitBuildOutputs && npm run origin && npm run heroku && npm run checkoutStag"
    },
    "dependencies": {
        "backbone": "^1.2.1",
